@model IEnumerable<SuperHeroes.Models.SuperHeroe>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlterEgo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrimarySuperHeroAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SeocondarySuperHeroeAbility)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Catchphrase)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlterEgo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrimarySuperHeroAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SeocondarySuperHeroeAbility)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Catchphrase)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
